<?xml version="1.0" encoding="utf-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"><!-- JSPC servlet mappings start -->

    <servlet>
        <servlet-name>org.apache.jsp.login_jsp</servlet-name>
        <servlet-class>org.apache.jsp.login_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.errorLogIn_jsp</servlet-name>
        <servlet-class>org.apache.jsp.errorLogIn_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.signup_jsp</servlet-name>
        <servlet-class>org.apache.jsp.signup_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.error_jsp</servlet-name>
        <servlet-class>org.apache.jsp.error_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.login_jsp</servlet-name>
        <url-pattern>/login.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.errorLogIn_jsp</servlet-name>
        <url-pattern>/errorLogIn.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.signup_jsp</servlet-name>
        <url-pattern>/signup.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.error_jsp</servlet-name>
        <url-pattern>/error.jsp</url-pattern>
    </servlet-mapping>

<!-- JSPC servlet mappings end -->

</web-app>